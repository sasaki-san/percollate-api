{
  "name": "percollate-api",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/main.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start:ts-node": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@nestjs/common": "^6.5.2",
    "@nestjs/core": "^6.5.2",
    "@nestjs/microservices": "^6.5.2",
    "@nestjs/platform-express": "^6.5.2",
    "@nestjs/testing": "^6.5.2",
    "@nestjs/websockets": "^6.5.2",
    "axios": "^0.19.0",
    "dist-exiftool": "^10.53.0",
    "helmet": "^3.21.2",
    "jsdom": "^13.2.0",
    "make-dir": "^1.3.0",
    "node-exiftool": "^2.3.0",
    "percollate": "github:yashha/percollate#epub",
    "readability": "github:mozilla/readability",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "turndown": "^5.0.3",
    "typescript": "^3.5.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.14.12",
    "@types/supertest": "^2.0.8",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.12.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "10.16.1",
    "npm": "5.6.x"
  }
}
